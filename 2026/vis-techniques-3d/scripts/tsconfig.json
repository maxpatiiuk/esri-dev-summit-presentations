  "include": ["*.ts"],
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "erasableSyntaxOnly": true,
    "target": "ES2024",
    "types": ["node"],
    "skipLibCheck": true
  }
}
